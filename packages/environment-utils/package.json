{
  "name": "@ai-code-agents/environment-utils",
  "version": "0.1.0",
  "description": "Utilities for implementing AI Code agents execution environments.",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "homepage": "https://github.com/felixarntz/ai-code-agents#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felixarntz/ai-code-agents.git",
    "directory": "packages/environment-utils"
  },
  "bugs": {
    "url": "https://github.com/felixarntz/ai-code-agents/issues"
  },
  "keywords": [
    "ai",
    "code",
    "agent",
    "environment",
    "utilities",
    "execution"
  ],
  "peerDependencies": {
    "ai": "^6.0.0",
    "zod": "^4.0.0"
  },
  "dependencies": {
    "@ai-sdk/provider-utils": "^4.0.0"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.3.3",
    "vitest": "^3.1.1",
    "zod": "4.3.4"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:ci": "eslint . --max-warnings 0",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "vitest run",
    "test:ci": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "publishConfig": {
    "access": "public"
  }
}
